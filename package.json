{
  "name": "ralph-ui",
  "version": "0.1.8",
  "type": "module",
  "description": "Orchestrate autonomous AI coding agents with the Ralph Wiggum Loop technique",
  "author": "Dario Valles",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dario-valles/Ralph-UI.git"
  },
  "homepage": "https://github.com/dario-valles/Ralph-UI#readme",
  "bugs": {
    "url": "https://github.com/dario-valles/Ralph-UI/issues"
  },
  "keywords": [
    "ai",
    "autonomous-agents",
    "claude",
    "claude-code",
    "coding-assistant",
    "developer-tools",
    "llm",
    "ralph-wiggum-loop"
  ],
  "bin": {
    "ralph-ui": "./bin/ralph-ui.js"
  },
  "files": [
    "bin/ralph-ui.js"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "server:build": "cd server && cargo build --release",
    "server": "cd server && cargo run --release -- --port 3421",
    "server:dev": "cd server && RUST_LOG=info cargo run -- --port 3421",
    "server:dev:token": "cd server && RALPH_SERVER_TOKEN=secure-token RUST_LOG=info cargo run -- --port 3421",
    "server:kill": "pkill -f 'ralph-ui' || true; lsof -ti:3421 | xargs kill -9 2>/dev/null || true",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "cargo:test": "cd server && cargo test",
    "cargo:build": "cd server && cargo build --release",
    "release:build": "bun run build && bun run cargo:build",
    "release:local": "bun run release:build && mkdir -p ~/.ralph-ui/bin && cp server/target/release/ralph-ui ~/.ralph-ui/bin/ && echo 'local-dev' > ~/.ralph-ui/version.txt && echo 'âœ“ Installed to ~/.ralph-ui/bin/ralph-ui'",
    "release:test": "bun run release:local && node bin/ralph-ui.js --skip-update",
    "release:pack": "bun run release:build && npm pack",
    "release:npx": "bun run release:local && node bin/ralph-ui.js --skip-update",
    "release:version": "npm version",
    "release:tag": "git push && git push --tags",
    "release:npm": "npm publish --access public"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@tailwindcss/vite": "^4.1.18",
    "@xterm/addon-fit": "^0.11.0",
    "@xterm/addon-web-links": "^0.12.0",
    "@xterm/addon-webgl": "^0.19.0",
    "@xterm/xterm": "^6.0.0",
    "@xyflow/react": "^12.10.0",
    "ansi-to-react": "^6.1.6",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.562.0",
    "motion": "^12.29.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.12.0",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.4.0",
    "vaul": "^1.1.2",
    "workbox-cacheable-response": "^7.4.0",
    "workbox-expiration": "^7.4.0",
    "workbox-routing": "^7.4.0",
    "workbox-strategies": "^7.4.0",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/eslint__js": "^9.14.0",
    "@types/node": "^25.0.9",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "^4.0.17",
    "@vitest/ui": "^4.0.17",
    "autoprefixer": "^10.4.23",
    "axe-core": "^4.11.1",
    "babel-plugin-react-compiler": "^1.0.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-compiler": "^19.1.0-rc.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "globals": "^17.0.0",
    "jest-axe": "^10.0.0",
    "jsdom": "^27.4.0",
    "postcss": "^8.5.6",
    "prettier": "^3.8.0",
    "rollup-plugin-visualizer": "^6.0.5",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.53.0",
    "vite": "^7.0.4",
    "vite-plugin-pwa": "^1.2.0",
    "vitest": "^4.0.17"
  }
}
